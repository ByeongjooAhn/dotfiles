# Custom Alias commands for ZSH

# Basic
alias cp='nocorrect cp -iv'
alias mv='nocorrect mv -iv'
alias rm='nocorrect rm -iv'

# Screen
alias scr='screen -rD'

# More Git aliases
# (overrides przto's git/alias.zsh)

alias gh='git history'
alias gha='gh --all'
alias gd='git diff'
alias gdc='gd --cached'
alias gs='git status'
alias gsu='gs -u'

